<query id="insertVideoThumbnail" action="insert">
	<tables>
		<table name="video_thumbnails" />
	</tables>
	<columns>
		<column name="file_srl" var="file_srl" filter="number" notnull="notnull" />
		<column name="document_srl" var="document_srl" filter="number" notnull="notnull" />
		<column name="video_url" var="video_url" notnull="notnull" />
		<column name="regdate" var="regdate" default="curdate()" />
	</columns>
</query>
